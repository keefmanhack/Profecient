<body>
	<div class="row">
		<div class="col-md-12">
			<%- include("../partials/flashMessage") %>
			<form action="/reset/<%=token%>/" method="POST">
				<legend>Reset Password</legend>
				<div class="form-group">
					<label for="username">New Password</label>
					<input type="password" name="password" autofocus class="form-control">
				</div>
				<div class="form-group">
					<label for="reenter-passworc">Retype Password</label>
					<input type="password" name="confirm" name="" class="form-control">
				</div>
				<div class="form-group">
					<input class="btn btn-primary" value="Reset Password" type="submit" name="">
				</div>
			</form>
		</div>
	</div>

